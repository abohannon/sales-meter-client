{
  "extends": "airbnb",
  "root": true,
  "parser": "babel-eslint",
  "globals": {
    "document": false
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "strict": 0,
    "react/jsx-filename-extension": ["error", {
      "extensions": [".js", ".jsx"]
    }],
    "react/forbid-prop-types": [0, {
      "ignore": ["object", "array"]
    }],
    "no-sequences": 0
  }
}
